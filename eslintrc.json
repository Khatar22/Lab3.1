{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": ["warn"],
    "no-undef": ["error"],
    "eqeqeq": ["error", "always"],
    "curly": ["error", "all"]
  }
}
